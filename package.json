{
  "name": "test-tasks",
  "homepage": "https://thevovchik.github.io/test-task",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix '*.cjs' './src/**/*.ts*'",
